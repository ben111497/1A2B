<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/lvBackground"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:lottie_autoPlay="true"
        app:lottie_loop="true"
        app:lottie_rawRes="@raw/bg_lottie" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.5"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imgTitle1"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="80dp"
        app:layout_constraintBottom_toBottomOf="@+id/cl5Words"
        app:layout_constraintEnd_toStartOf="@+id/imgTitleA"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.19999999"
        app:srcCompat="@drawable/title_1" />

    <ImageView
        android:id="@+id/imgTitleA"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="4dp"
        app:layout_constraintEnd_toStartOf="@+id/imgTitle2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imgTitle1"
        app:layout_constraintTop_toTopOf="@+id/imgTitle1"
        app:srcCompat="@drawable/title_a" />

    <ImageView
        android:id="@+id/imgTitle2"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="4dp"
        app:layout_constraintEnd_toStartOf="@+id/imgTitleB"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imgTitleA"
        app:layout_constraintTop_toTopOf="@+id/imgTitleA"
        app:srcCompat="@drawable/title_2" />

    <ImageView
        android:id="@+id/imgTitleB"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="4dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imgTitle2"
        app:layout_constraintTop_toTopOf="@+id/imgTitle2"
        app:srcCompat="@drawable/title_b" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cl4Words"
        android:layout_width="0dp"
        android:layout_height="75dp"
        android:background="@drawable/rp_rectangle_f2efd37d_20"
        app:layout_constraintBottom_toTopOf="@+id/cl5Words"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgTitle1"
        app:layout_constraintVertical_bias="0.19999999"
        app:layout_constraintVertical_chainStyle="packed"
        app:layout_constraintWidth_percent=".7">

        <TextView
            android:id="@+id/tv4WordStatus"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="@drawable/rp_rectangle_f2f1b948_20_right"
            android:gravity="center"
            android:paddingStart="4dp"
            android:paddingEnd="4dp"
            android:text="Unlock"
            android:textColor="@color/white_FFFFFF"
            android:textSize="20dp"

            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintWidth_percent=".3" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:shadowColor="@color/gray_9F9F9F"
            android:shadowDx="2"
            android:shadowDy="2"
            android:shadowRadius="5"
            android:text="4 Words"
            android:textColor="@color/white_FFFFFF"
            android:textSize="32dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/tv4WordStatus"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cl5Words"
        android:layout_width="0dp"
        android:layout_height="75dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/rp_rectangle_d85c5c5c_20"
        app:layout_constraintBottom_toTopOf="@+id/cl6Words"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cl4Words"
        app:layout_constraintWidth_percent=".7">

        <TextView
            android:id="@+id/tv5WordStatus"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="@drawable/rp_rectangle_d83e3a34_20_right"
            android:gravity="center"
            android:paddingStart="4dp"
            android:paddingEnd="4dp"
            android:text="Lock"
            android:textColor="@color/white_FFFFFF"
            android:textSize="20dp"

            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintWidth_percent=".3" />

        <TextView
            android:id="@+id/textView12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:shadowColor="@color/gray_9F9F9F"
            android:shadowDx="2"
            android:shadowDy="2"
            android:shadowRadius="5"
            android:text="5 Words"
            android:textColor="@color/white_FFFFFF"
            android:textSize="32dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/tv5WordStatus"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cl6Words"
        android:layout_width="0dp"
        android:layout_height="75dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/rp_rectangle_d85c5c5c_20"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cl5Words"
        app:layout_constraintWidth_percent=".7">

        <TextView
            android:id="@+id/tv6WordStatus"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="@drawable/rp_rectangle_d83e3a34_20_right"
            android:gravity="center"
            android:paddingStart="4dp"
            android:paddingEnd="4dp"
            android:text="Lock"
            android:textColor="@color/white_FFFFFF"
            android:textSize="20dp"

            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintWidth_percent=".3" />

        <TextView
            android:id="@+id/textView13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:shadowColor="@color/gray_9F9F9F"
            android:shadowDx="2"
            android:shadowDy="2"
            android:shadowRadius="5"
            android:text="6 Words"
            android:textColor="@color/white_FFFFFF"
            android:textSize="32dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/tv6WordStatus"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>